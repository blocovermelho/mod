{
	"schema_version": 1,
	"quilt_loader": {
		"group": "org.blocovermelho",
		"id": "bv-quilt",
		"version": "${version}",
		"metadata": {
			"name": "BV Mod",
			"description": "Mod do servidor de minecraft do Bloco Vermelho.",
			"contributors": {
				"roridev": "Owner"
			},
			"contact": {
				"homepage": "https://blocovermelho.org/",
				"issues": "https://github.com/blocovermelho/mod/issues",
				"sources": "https://github.com/blocovermelho/mod"
			},
			"icon": "assets/example_mod/icon.png"
		},
		"intermediate_mappings": "net.fabricmc:intermediary",
		"entrypoints": {
			"init": {
				"adapter": "kotlin",
				"value": "org.blocovermelho.mod.BVQuilt"
			}
		},
		"depends": [
			{
				"id": "quilt_loader",
				"versions": ">=0.19.2"
			},
			{
				"id": "quilted_fabric_api",
				"versions": ">=7.0.2"
			},
			{
				"id": "qkl",
				"versions": ">=2.1.0"
			},
			{
				"id": "minecraft",
				"versions": ">=1.20"
			}
		]
	},
	"mixin": "bv.mixins.json"
